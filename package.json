{
  "name": "nbp_redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.0.17",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-live-clock": "^2.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "recharts": "^1.0.0-beta.10",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "throttle-debounce": "^1.0.1",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-front": "SET REACT_APP_NODE_ENV=development && npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-js": "react-scripts build",
    "postinstall": "SET REACT_APP_NODE_ENV=production npm-run-all build-css build-js",
    "devbuild": "SET REACT_APP_NODE_ENV=development && npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "proxy": "http://localhost:8000/",
    "start": "SET NODE_ENV=production && cd server && node server.js",
    "server": "SET NODE_ENV=development && cd server && nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm start-front\""
  }
}
